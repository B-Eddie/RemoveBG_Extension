{
  "name": "CopyBG",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Extension that lets you copy images without the bg",
  "icons": {
    "48": "icon.png"
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": ["action.js"],
    "persistent": true
  },
  "permissions": ["contextMenus", "storage", "clipboardWrite", "activeTab"],
  "default_locale": "en"
}
